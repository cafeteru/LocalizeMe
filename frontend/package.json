{
  "name": "localize-me-front",
  "version": "1.0.2",
  "scripts": {
    "build": "ng build",
    "codecov": "codecov",
    "coverage-watch": "ng test --code-coverage",
    "coverage": "ng test --no-watch --code-coverage",
    "deploy": "ng build --configuration production && firebase deploy",
    "e2e-open": "npx cypress open",
    "e2e-run": "npx cypress run",
    "format": "prettier --write src/**/*.{ts,html,scss}",
    "install-ci": "yarn install --frozen-lockfile",
    "lint": "ng lint",
    "ng": "ng",
    "pre-commit": "npm run format && npm run lint && npm run unit-test",
    "start": "ng serve",
    "test": "ng test --code-coverage",
    "unit-test": "ng test --no-watch --code-coverage --browsers=ChromeHeadless",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@ant-design/icons-angular": "14.1.0",
    "@ngrx/effects": "^14.3.3",
    "@ngrx/store": "^14.3.3",
    "@ngrx/store-devtools": "^14.3.3",
    "file-saver": "^2.0.5",
    "firebase": "^9.21.0",
    "jwt-decode": "^3.1.2",
    "ng-zorro-antd": "^14.3.0",
    "rxjs": "~7.5.7",
    "tslib": "^2.5.0",
    "xml-beautifier": "^0.5.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.11",
    "@angular-eslint/builder": "^14.4.0",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "^14.2.11",
    "@angular/compiler-cli": "^14.3.0",
    "@types/jasmine": "~4.0.3",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "cypress": "^9.7.0",
    "eslint": "^8.39.0",
    "jasmine-core": "~4.1.1",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-mocha-reporter": "^2.2.5",
    "prettier": "2.6.2",
    "typescript": "~4.6.4"
  }
}
